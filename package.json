{
	"name": "vscode-api-client",
	"displayName": "VS Code API Client",
	"description": "A feature-rich API client for Visual Studio Code, similar to Postman.",
	"publisher": "wizard",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "apiClient",
					"title": "API Client",
					"icon": "$(globe)"
				}
			]
		},
		"views": {
			"apiClient": [
				{
					"id": "apiClient.sidebar",
					"name": "Sidebar",
					"type": "webview",
					"icon": "$(globe)"
				}
			]
		},
		"commands": [],
		"menus": {}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"precompile": "rimraf dist",
		"compile": "webpack",
		"prewatch": "rimraf dist",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts,tsx",
		"lint:fix": "eslint src --ext ts,tsx --fix",
		"test": "jest --passWithNoTests --silent",
		"test:unit": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:vscode": "npm run compile-tests && vscode-test",
		"test:verbose": "jest --verbose"
	},
	"devDependencies": {
		"@prettier/plugin-xml": "^3.4.2",
		"@svgr/webpack": "^8.1.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/typography": "^0.5.19",
		"@types/lodash.throttle": "^4.1.9",
		"@types/marked": "^5.0.2",
		"@types/mime-types": "^3.0.1",
		"@types/node": "20.x",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@types/vscode": "^1.70.0",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"@vscode/test-cli": "^0.0.12",
		"@vscode/test-electron": "^2.3.9",
		"autoprefixer": "^10.4.21",
		"copy-webpack-plugin": "^13.0.1",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.3",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"file-loader": "^6.2.0",
		"mini-css-extract-plugin": "^2.9.2",
		"monaco-editor-webpack-plugin": "^7.1.1",
		"node-loader": "^2.1.0",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.6.2",
		"rimraf": "^6.1.2",
		"sass": "^1.94.2",
		"sass-embedded": "^1.93.3",
		"sass-loader": "^16.0.6",
		"source-map-loader": "^5.0.0",
		"style-loader": "^4.0.0",
		"tailwindcss": "^4.1.11",
		"tailwindcss-animate": "^1.0.7",
		"terser-webpack-plugin": "^5.3.14",
		"ts-loader": "^9.5.4",
		"tw-animate-css": "^1.3.6",
		"typescript": "~5.5.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
